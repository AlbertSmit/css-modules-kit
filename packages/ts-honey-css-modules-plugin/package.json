{
  "name": "ts-honey-css-modules-plugin",
  "description": "TypeScript Language Service Plugin for CSS Modules",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mizdra/honey-css-modules.git",
    "directory": "packages/ts-honey-css-modules-plugin"
  },
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": true,
  "exports": {
    ".": {
      "default": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build-watch": "npm run build -- --watch --preserveWatchOutput"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "css-modules",
    "typescript"
  ],
  "files": [
    "bin",
    "src",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "!src/test",
    "dist"
  ],
  "peerDependencies": {
    "typescript": ">=5.6.3"
  }
}
